#include "Local.config"

MsubTarget(xpilot.man,xpilot.man.msub)
MsubTarget(xpilots.man,xpilots.man.msub)
MsubTarget(xp-replay.man,xp-replay.man.msub)
MsubDepend

InstallManPage(xpilot,$(INSTMANDIR))
